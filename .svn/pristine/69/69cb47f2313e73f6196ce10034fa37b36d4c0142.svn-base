<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/colorAccent_</item>
        <item name="colorPrimary">@color/colorPrimary_</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark_</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:typeface">sans</item>
        <item name="cardViewStyle">@style/CardView</item>
    </style>

    <style name="AppTheme.NoActionBar" parent="@style/AppTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppLoginTheme" parent="Theme.AccountKit">
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="html_theme" parent="android:Theme.Light" />

    <style name="fbbtn">
        <item name="android:textSize">8pt</item>
        <item name="android:background">@drawable/btn_facebook_press</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="com_facebook_button" parent="@android:style/Widget.Button">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/com_facebook_button_background</item>
        <item name="android:paddingLeft">8.0dip</item>
        <item name="android:paddingTop">5.0dip</item>
        <item name="android:paddingRight">8.0dip</item>
        <item name="android:paddingBottom">5.0dip</item>
        <item name="android:drawablePadding">6.0dip</item>
    </style>


    <style name="editTextStyle">
        <item name="android:layout_height">55dp</item>
        <item name="android:textColorHint">@color/aqua</item>
        <item name="android:background">@drawable/img_edittext</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:textColor">@color/future_gray</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="btnStyleBeige" parent="@android:style/Widget.Button">
        <item name="android:textSize">15dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:gravity">center</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <!-- <item name="android:shadowDy">1</item> -->
        <item name="android:shadowRadius">0.6</item>
        <item name="android:background">@drawable/button_selector</item>
        <item name="android:padding">10dip</item>
    </style>

    <style name="FacebookLoginButton">

        <!-- <item name="android:textSize">@dimen/smallTxtSize</item> -->
        <item name="android:background">@android:color/holo_green_dark</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="slideBottomTopTheme" parent="@android:style/Theme.Panel">
        <item name="android:windowAnimationStyle">@style/slideBottomTopTheme.Window</item>
    </style>

    <style name="slideTopBottomTheme" parent="@android:style/Theme.Panel">
        <item name="android:windowAnimationStyle">@style/slideTopBottomTheme.Window</item>
    </style>

    <style name="slideBottomTopTheme.Window" parent="@android:style/Animation.Activity">
        <item name="android:windowEnterAnimation">@anim/abc_slide_in_bottom</item>
        <item name="android:windowExitAnimation">@anim/abc_slide_out_bottom</item>
    </style>

    <style name="slideTopBottomTheme.Window" parent="@android:style/Animation.Activity">
        <item name="android:windowEnterAnimation">@anim/abc_slide_in_top</item>
        <item name="android:windowExitAnimation">@anim/abc_slide_out_top</item>
    </style>

    <style name="SeekArc">
        <item name="arcColor">@color/progress_gray_dark</item>
    </style>

    <style name="SeekArcLight">
        <item name="arcColor">@color/progress_gray</item>
    </style>

    <style name="profilertextstyle">
        <item name="android:textSize">6pt</item>
        <item name="android:textColor">#4C4C4C</item>
    </style>

    <style name="boldtextstyle">
        <item name="android:textSize">8pt</item>
        <item name="android:textColor">@color/gray_opacity</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="textview_info_oval">
        <item name="android:layout_width">10dip</item>
        <item name="android:layout_height">10dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">8sp</item>
        <item name="android:paddingTop">0dip</item>
        <item name="android:paddingBottom">1dip</item>
        <item name="android:paddingLeft">1dip</item>
        <item name="android:paddingRight">1dip</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/info_red</item>
    </style>


    <style name="TransparentDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>

    <style name="Theme_Dialog" parent="android:Theme.Holo.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowSoftInputMode">adjustResize|stateHidden</item>
    </style>

    <style name="AppTheme.NoActionBar.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.NoActionBar.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.NoActionBar.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/enter_from_left</item>
        <item name="android:windowExitAnimation">@anim/exit_to_right</item>
    </style>


</resources>