package com.panelManagement.model;

import java.io.Serializable;

public class GlobalSettingsModel implements Serializable {
    private static final long serialVersionUID = 1L;
    int campaignId = 0;
    String cAMPAIGNNAME = "";
    String iPLogging = "";
    String captcha = "";
    String xGEOLocking = "";
    String globalSettingName = "";
    String sequence = "";
    String visible = "";


    public GlobalSettingsModel(int id, String cAMPAIGNNAME, String iPLogging,
                               String captcha, String xGEOLocking, String globalSettingName,
                               String sequence, String visible) {
        this.campaignId = id;
        this.cAMPAIGNNAME = cAMPAIGNNAME;
        this.iPLogging = iPLogging;
        this.captcha = captcha;
        this.xGEOLocking = xGEOLocking;
        this.globalSettingName = globalSettingName;
        this.sequence = sequence;
        this.visible = visible;
    }

    public int getCampaignId() {
        return campaignId;
    }

    public void setCampaignId(int id) {
        this.campaignId = id;
    }

    public String getcAMPAIGNNAME() {
        return cAMPAIGNNAME;
    }

    public void setcAMPAIGNNAME(String cAMPAIGNNAME) {
        this.cAMPAIGNNAME = cAMPAIGNNAME;
    }

    public String getiPLogging() {
        return iPLogging;
    }

    public void setiPLogging(String iPLogging) {
        this.iPLogging = iPLogging;
    }

    public String getCaptcha() {
        return captcha;
    }

    public void setCaptcha(String captcha) {
        this.captcha = captcha;
    }

    public String getxGEOLocking() {
        return xGEOLocking;
    }

    public void setxGEOLocking(String xGEOLocking) {
        this.xGEOLocking = xGEOLocking;
    }

    public String getGlobalSettingName() {
        return globalSettingName;
    }

    public void setGlobalSettingName(String globalSettingName) {
        this.globalSettingName = globalSettingName;
    }

    public String getSequence() {
        return sequence;
    }

    public void setSequence(String sequence) {
        this.sequence = sequence;
    }

    public String getVisible() {
        return visible;
    }

    public void setVisible(String visible) {
        this.visible = visible;
    }

}
