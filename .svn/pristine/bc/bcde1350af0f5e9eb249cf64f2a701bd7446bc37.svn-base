package com.panelManagement.model;

import java.util.ArrayList;
import java.util.List;

public class QuickPollQuestionItem {

    private Integer questionID;
    private String questionName;
    private Integer questionTypeID;
    private ArrayList<OpinionPollsAnswerChoice> opinionPollsAnswerChoices = null;

    public QuickPollQuestionItem(Integer questionID, String questionName, Integer questionTypeID, ArrayList<OpinionPollsAnswerChoice> opinionPollsAnswerChoices) {
        this.questionID = questionID;
        this.questionName = questionName;
        this.questionTypeID = questionTypeID;
        this.opinionPollsAnswerChoices = opinionPollsAnswerChoices;
    }


    public Integer getQuestionID() {
        return questionID;
    }

    public void setQuestionID(Integer questionID) {
        this.questionID = questionID;
    }

    public String getQuestionName() {
        return questionName;
    }

    public void setQuestionName(String questionName) {
        this.questionName = questionName;
    }

    public Integer getQuestionTypeID() {
        return questionTypeID;
    }

    public void setQuestionTypeID(Integer questionTypeID) {
        this.questionTypeID = questionTypeID;
    }

    public List<OpinionPollsAnswerChoice> getOpinionPollsAnswerChoices() {
        return opinionPollsAnswerChoices;
    }

    public void setOpinionPollsAnswerChoices(ArrayList<OpinionPollsAnswerChoice> opinionPollsAnswerChoices) {
        this.opinionPollsAnswerChoices = opinionPollsAnswerChoices;
    }

}